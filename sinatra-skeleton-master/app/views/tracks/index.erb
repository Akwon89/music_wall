<h1>Track Listing</h1>
<hr/>

<% @tracks.each do |track| %>
  <div class="track">
    <h3><%= track.song_title %> <small>(by: <%= track.author %>)</small></h3>
    <p>
      <%= track.url %>
    </p>
  </div>
  <hr/>
<% end %>

<hr/>
<a href="/tracks/new">Post a track... It's FREE (for now)!</a>
<hr/>
